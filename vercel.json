{
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    
    "routes": [
      { "src": "/(.*)", "dest": "/server/index.js" },
      {
        "src": "/auth",
        "dest": "./routes/auth.js"
      },
      {
        "src": "/(.*)",
        "dest": "./routes/home.js"
      }
    ]
  }
  
  